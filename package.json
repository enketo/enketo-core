{
    "name": "enketo-core",
    "description": "Extensible Enketo form engine",
    "homepage": "https://enketo.org",
    "version": "5.6.0",
    "license": "Apache-2.0",
    "os": [
        "darwin",
        "linux",
        "win32"
    ],
    "keywords": [
        "enketo",
        "OpenRosa",
        "ODK",
        "XForms"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/enketo/enketo-core"
    },
    "author": {
        "name": "Martijn van de Rijdt",
