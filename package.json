        "docdash": "^1.0.3",
        "enketo-transformer": "1.33.0",
        "eslint-plugin-jsdoc": "15.8.x",
        "grunt": "^1.0.4",
        "grunt-concurrent": "3.x.x",
        "grunt-contrib-connect": "2.0.x",
        "grunt-contrib-watch": "1.x.x",
        "grunt-eslint": "22.0.x",
        "grunt-jsbeautifier": "0.x.x",
        "grunt-karma": "3.0.2",
        "grunt-sass": "^3.0.2",
        "grunt-shell": "3.0.x",
